disable_cache = true
disable_mlock = true
ui = true
listener "tcp" {
    address = "0.0.0.0:8200"
    tls_disable = 0
    tls_cert_file = "/etc/letsencrypt/live/{{ vault_domain }}/fullchain.pem"
    tls_key_file = "/etc/letsencrypt/live/{{ vault_domain }}/privkey.pem"
}
storage "file" {
  path = "/etc/{{ vault_work_dir }}/data"
}
max_lease_ttl = "10h"
default_lease_ttl = "10h"
